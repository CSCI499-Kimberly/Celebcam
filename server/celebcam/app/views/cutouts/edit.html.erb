<h1>Editing cutout</h1>





<%= form_for(@cutout) do |f| %>
  <% if @cutout.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@cutout.errors.count, "error") %> prohibited this cutout from being saved:</h2>

      <ul>
      <% @cutout.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :image_link %><br />
