<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="slidemenu_btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/slidemenu_btn_text_color</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/slidemenu_btn_bg</item>
    </style>
    <style name="handle_btn">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:src">@drawable/littlestar</item>
        <item name="android:background">@drawable/slidemenu_handle_bg</item>
        <item name="android:layout_gravity">left</item>
        
    </style>
</resources>